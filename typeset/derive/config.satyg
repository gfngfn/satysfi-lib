@import: line

type label-position =
    | LabelLeft
    | LabelRight

type derive-config = (|
    default-label-position : label-position;
    default-line : derivation-line;
    bar-top-bottom-margin : length;
    label-left-right-margin : length;
    bar-thickness : length;
    content-left-right-padding : length;
|)
