@require: math
@import: ../util
@import: ../../../typeset/derive
@import: ../../../typeset/derive/line

open Derive in
let my-derive-config = open DeriveConfig in
    default
    |> with-default-label-position LabelRight
    |> with-default-line (DeriveLine.dotted-line 5pt 5pt)
    |> with-bar-top-bottom-margin 10pt
    |> with-label-left-right-margin 10pt
    |> with-bar-thickness 10pt 
    |> with-content-left-right-padding 30pt
in
satysfi-it ${
  \proven ?:!(my-derive-config) !(
    open DeriveDSL in
    derive ${ \Gamma \vdash \Delta }
    |> by {Cut}
    |> from [
      assume ${\Gamma \vdash \Delta, A};
      assume ${\Gamma, A \vdash \Delta}
    ]
  )
}

