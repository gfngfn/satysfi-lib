@require: stdja
@import: ../../../typeset/derive

open Derive in
StdJa.document (|
  title = {はじめての\SATySFi;};
  author = {オレオレ};
  show-title = true;
  show-toc = false;
|) '<
  +math (${
    \proven!(
      open DeriveDSL in
      derive ${ \Gamma \vdash \Delta }
      |> by {Cut}
      |> withLine (dotted-line 2pt 1pt)
